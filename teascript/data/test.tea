PRINT(15, test())
PRINT(16, test()+test())

VARIABLE(n, 9)
PRINT(F, fib())
END()

FDEF(fib())
  BRANCH(n > 1, fancy)
  VARIABLE(ans, n)
  GOTO(fibend)
  
  LABEL(fancy)
  VARIABLE(n1, n-1)
  VARIABLE(n2, n-2)
  
  VARIABLE(n, n1)
  VARIABLE(p1, fib())
  VARIABLE(n, n2)
  VARIABLE(p2, fib())
  VARIABLE(ans, p1 + p2)

  LABEL(fibend)
EFDEF()

FDEF(test())
  VARIABLE(ans, 2.5)
EFDEF()