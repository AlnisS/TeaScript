VARIABLE(n, 9)
VARIABLE(ans, 0.1)
USERFUN(fib())
PRINT(F, ans)
END()

FDEF(fib())
  BRANCH(n > 1, fancy)
  VARSET(ans, n)
  GOTO(fibend)
  LABEL(fancy)
  #return fib(n-1) + fib(n-2);
  VARIABLE(n, n-1)
  VARIABLE(ans, 0)
  USERFUN(fib())
  VARIABLE(ansb, ans)
  REMVAR(n)
  VARIABLE(n, n-2)
  USERFUN(fib())
  VARSET(ans, ans+ansb)
  LABEL(fibend)
EFDEF()
