FDEF(init())
  RET(0)
EFDEF()

FDEF(main())
  
  VARIABLE(acc, 0)
  VARIABLE(a, 0)
  VARIABLE(b, 1)
  VARIABLE(c, 0)
  WHILE(b < 4000000)
    IF(b % 2 == 0)
      VARIABLE(acc, acc + b)
    ENDIF()
    VARIABLE(c, a + b)
    VARIABLE(a, b)
    VARIABLE(b, c)
  ENDWHILE()
  PRINT(acc)
  
  END()
  RET(0)
EFDEF()
